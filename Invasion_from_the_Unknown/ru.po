#. [side]: type=Shadow Courier, id=An-Laila
#: Invasion_from_the_Unknown/scenarios/23C_Broken_Heart.cfg:39
#: Invasion_from_the_Unknown/scenarios/23B_Until_Death.cfg:189
#: Invasion_from_the_Unknown/scenarios/23B_Until_Death.cfg:245
#: Invasion_from_the_Unknown/scenarios/20_The_Heart.cfg:361
#, fuzzy
msgid "team_name^Uria"
msgstr "Нежить"

#. [side]
#: Invasion_from_the_Unknown/scenarios/23C_Broken_Heart.cfg:61
#: Invasion_from_the_Unknown/scenarios/23C_Broken_Heart.cfg:147
#: Invasion_from_the_Unknown/scenarios/23C_Broken_Heart.cfg:182
#: Invasion_from_the_Unknown/scenarios/22B_Dark_Hive.cfg:122
